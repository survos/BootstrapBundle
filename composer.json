{
    "name": "survos/bootstrap-bundle",
    "description": "Integrates a simple bootstrap theme in a Symfony application",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Tac Tacelosky",
            "email": "tacman@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "knplabs/knp-menu-bundle": "^3.2",
        "symfony/config": "^6.1.0",
        "symfony/dependency-injection": "^6.1.0",
        "symfony/http-kernel": "^6.1.0",
        "symfony/options-resolver": "^6.1.0",
        "symfony/security-bundle": "^6.1.0",
        "symfony/ux-twig-component": "^2.3",
        "twig/string-extra": "^3.4",
        "twig/twig": "^3.4",
        "survos/core-bundle": "^1.2.14"
    },
    "autoload": {
        "psr-4": {
            "Survos\\BootstrapBundle\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.3-dev"
        },
        "symfony": {
            "require": "^6.1.0"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "phpstan": "vendor/bin/phpstan analyse"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.8"
    },
    "conflict": {
        "survos/maker-bundle": "<1.2.14",
        "survos/html-prettify-bundle": "<1.2.14",
        "survos/barcode-bundle": "<1.2.14",
        "survos/api-grid-bundle": "<1.2.14",
        "survos/second-package": "<1.2.7",
        "survos/inspection-bundle": "<1.2.14",
        "survos/faker-bundle": "<1.2.14",
        "survos/doc-bundle": "<1.2.14",
        "survos/grid-bundle": "<1.2.14",
        "survos/tree-bundle": "<1.2.14",
        "survos/first-package": "<1.2.7",
        "survos/auth-bundle": "<1.2.14",
        "survos/workflow-helper-bundle": "<1.2.14",
        "survos/crawler-bundle": "<1.2.14",
        "survos/location-bundle": "<1.2.14"
    }
}
